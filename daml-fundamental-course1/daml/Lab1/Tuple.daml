module Tuple where

import Daml.Script

import DA.Tuple (thd3, snd3, fst3)

testTuple: Script()

testTuple = script do
    let 
        twoTuple = ("abc", 123)
        threeTuple = ("a", 123, True)
        nTuple = (1, "hello", True, [1,2])

    debug $ fst twoTuple    
    debug $ snd twoTuple
    
    debug $ fst3 threeTuple
    debug $ snd3 threeTuple
    debug $ thd3 threeTuple

    debug $ nTuple._1
    debug nTuple._4

    




